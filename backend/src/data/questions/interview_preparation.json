            "id": "INT001",
            "number": 701,
            "title": "Two Sum Problem",
            "difficulty": "Easy",
            "points": 10,
            "description": "Given an array of integers and a target sum, find two distinct indices such that their elements add up to the target.\n\nReturn indices (0-indexed). If multiple solutions exist, return any one. If no solution, print -1.",
            "inputFormat": "First line: Two integers N and target.\nSecond line: N space-separated integers.",
            "outputFormat": "Print two space-separated indices or -1.",
            "constraints": "2 <= N <= 10^5\n-10^9 <= elements, target <= 10^9",
            "sampleInput": "4 9\n2 7 11 15",
            "sampleOutput": "0 1",
            "explanation": "nums[0] + nums[1] = 2 + 7 = 9.",
            "testCases": [
                {
                    "input": "4 9\n2 7 11 15",
                    "expectedOutput": "0 1",
                    "visibility": "public"
                },
                {
                    "input": "3 6\n3 2 4",
                    "expectedOutput": "1 2",
                    "visibility": "public"
                },
                {
                    "input": "2 10\n5 5",
                    "expectedOutput": "0 1",
                    "visibility": "hidden"
                },
                {
                    "input": "4 100\n1 2 3 4",
                    "expectedOutput": "-1",
                    "visibility": "hidden"
                },
                {
                    "input": "5 0\n-1 0 1 2 -2",
                    "expectedOutput": "0 2",
                    "visibility": "hidden"
                },
                {
                    "input": "6 -5\n-3 -2 1 5 -1 0",
                    "expectedOutput": "0 1",
                    "visibility": "hidden"
                }
            ]
        },
