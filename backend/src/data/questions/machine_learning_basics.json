            "testCases": [
                {
                    "input": "3\n1 2 3\n4 5 6",
                    "expectedOutput": "5.20",
                    "visibility": "public"
                },
                {
                    "input": "2\n0 0\n3 4",
                    "expectedOutput": "5.00",
                    "visibility": "public"
                },
                {
                    "input": "1\n0\n10",
                    "expectedOutput": "10.00",
                    "visibility": "hidden"
                },
                {
                    "input": "4\n1 1 1 1\n2 2 2 2",
                    "expectedOutput": "2.00",
                    "visibility": "hidden"
                }
            ]
        },
        {
