        {
            "id": "ML001",
            "number": 1401,
            "title": "Mean and Standard Deviation",
            "difficulty": "Easy",
            "points": 10,
            "description": "Calculate the mean and standard deviation of a given dataset.\n\nUse the population standard deviation formula.",
            "inputFormat": "First line: N.\nSecond line: N space-separated numbers.",
            "outputFormat": "Mean and standard deviation (2 decimal places).",
            "constraints": "1 <= N <= 10^5\n-10^6 <= values <= 10^6",
            "sampleInput": "5\n10 20 30 40 50",
            "sampleOutput": "30.00 14.14",
            "explanation": "Mean = 30. Variance = 200. StdDev = √200 ≈ 14.14.",
            "testCases": [
                {
                    "input": "5\n10 20 30 40 50",
                    "expectedOutput": "30.00 14.14",
                    "visibility": "public"
                },
                {
                    "input": "4\n2 4 6 8",
                    "expectedOutput": "5.00 2.24",
                    "visibility": "public"
                },
                {
                    "input": "1\n100",
                    "expectedOutput": "100.00 0.00",
                    "visibility": "hidden"
                },
                {
                    "input": "3\n-10 0 10",
                    "expectedOutput": "0.00 8.16",
                    "visibility": "hidden"
                }
            ]
        },
        {
